{
  "name": "_blank",
  "version": "1.0.0",
  "description": "Yet another React-based web application starter kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/froloff/_blank.git"
  },
  "scripts": {
    "start": "yarn build && cross-env NODE_ENV=production node server/index.js",
    "build": "rimraf build && cross-env NODE_ENV=production webpack -p",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "eslint": "eslint --cache --ext .jsx,.js --ignore-path .gitignore ./",
    "stylelint": "stylelint client/**/*.pcss --cache",
    "lint": "yarn eslint && yarn stylelint",
    "precommit": "yarn lint"
  },
  "author": "Leonid Frolov <leonid@froloff.me> (https://github.com/froloff)",
  "license": "CC0-1.0",
  "dependencies": {
    "autoprefixer": "^7.1.6",
    "axios": "^0.17.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "json-proxy": "^0.9.3",
    "normalize.css": "^7.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-nested": "^2.1.2",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.1.2",
    "prop-types": "^15.6.0",
    "pug": "^2.0.0-rc.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.19.0",
    "svg-react-loader": "^0.4.5",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "nodemon": "^1.12.1",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  }
