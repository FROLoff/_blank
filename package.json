{
  "name": "_blank",
  "version": "0.0.1",
  "description": "Yet another React-based web application starter kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/froloff/_blank.git"
  },
  "scripts": {
    "start": "NODE_ENV=development node server/index.js",
    "start:production": "NODE_ENV=production node server/index.js",
    "clean": "rimraf public",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack --config config/webpack.production.js"
  },
  "author": "Leonid Frolov <leonid@froloff.me> (https://github.com/froloff)",
  "license": "CC0-1.0",
  "dependencies": {
    "express": "^4.13.4",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.8.1",
    "normalize.css": "^3.0.3",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.0",
    "postcss-url": "^5.1.1",
    "react-transform-hmr": "^1.0.2",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "env": {
      "development": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  }
}
