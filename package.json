{
  "name": "_blank",
  "version": "1.0.0",
  "description": "Yet another React-based web application starter kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/froloff/_blank.git"
  },
  "scripts": {
    "start": "yarn build && cross-env NODE_ENV=production node server/index.js",
    "build": "rimraf build && cross-env NODE_ENV=production webpack -p",
    "dev": "nodemon server/index.js --watch server",
    "eslint": "eslint --cache --ext .jsx,.js --ignore-path .gitignore ./",
    "stylelint": "stylelint client/**/*.pcss --cache",
    "lint": "yarn eslint && yarn stylelint",
    "precommit": "yarn lint"
  },
  "author": "Leonid Frolov <leonid@froloff.me> (https://github.com/froloff)",
  "license": "CC0-1.0",
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "json-proxy": "^0.9.3",
    "mini-css-extract-plugin": "^0.9.0",
    "normalize.css": "^8.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prop-types": "^15.7.2",
    "pug": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "svg-react-loader": "^0.4.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
