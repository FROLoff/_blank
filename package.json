{
  "name": "_blank",
  "version": "0.2.0",
  "description": "Yet another React-based web application starter kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/froloff/_blank.git"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon --watch server server/index.js",
    "build": "rimraf build && NODE_ENV=production webpack -p",
    "debug": "NODE_ENV=development node-nightly --inspect --debug-brk server/index.js",
    "eslint": "eslint --cache --ext .jsx,.js --ignore-path .gitignore ./",
    "stylelint": "stylelint --cache client/**/*.pcss",
    "lint": "yarn run eslint && yarn run stylelint",
    "precommit": "yarn run lint"
  },
  "author": "Leonid Frolov <leonid@froloff.me> (https://github.com/froloff)",
  "license": "CC0-1.0",
  "dependencies": {
    "axios": "^0.16.1",
    "classnames": "^2.2.5",
    "express": "^4.15.2",
    "json-proxy": "^0.9.3",
    "nodemon": "^1.11.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-es2015": "^6.24.1",
