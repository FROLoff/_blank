{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
      "browser": true,
      "node": true
  },
  "rules": {
    "global-require": 0,
    "class-methods-use-this": 0,
    "import/newline-after-import": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "react/prefer-stateless-function": 0,
    "react/forbid-prop-types": 0
  }
}
